<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CustoViagemMaui.Views.CalculoFinal"
             Title="CalculoFinal">
    <VerticalStackLayout>
        <Grid ColumnDefinitions="*, *" RowDefinitions="*,*">
            <Label Text="Origem" Grid.Column="0" Grid.Row="0"/>
            <Label Text="{Binding Origem}" Grid.Column="1" Grid.Row="0"/>
            <Label Text="Destino" Grid.Column="0" Grid.Row="1"/>
        </Grid>
    </VerticalStackLayout>
</ContentPage>